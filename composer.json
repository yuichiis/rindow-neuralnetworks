{
    "name": "rindow/rindow-neuralnetworks",
    "type": "library",
    "description": "Neural networks library for deep learning",
    "keywords": ["rindow","neural","networks","deep learning","deep","learning","machine learning"],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "rindow/rindow-math-matrix": "~2.0.0"
    },
    "require-dev": {
        "rindow/rindow-math-plot": "~1.0.8",
        "ext-pdo_sqlite": "*"
    },
    "suggest": {
        "rindow/rindow-math-plot": "when visualize machine learning results",
        "ext-pdo_sqlite": "for save model",
        "rindow-math-buffer-ffi": "1.0.0 strongly recommended for speed",
        "rindow-openblas-ffi": "1.0.0 strongly recommended for speed",
        "rindow-opencl-ffi": "1.0.0 GPU/OpenCL support",
        "rindow-clblast-ffi": "1.0.0 GPU/OpenCL support"
    },
    "autoload": {
        "psr-4": {
            "Rindow\\NeuralNetworks\\": "src/"
        }
    }
}
